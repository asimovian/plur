{
    "tags": {
        "allowUnknownTags": false,
	"dictionaries": ["jsdoc","closure"]
    },
    "source": {
        "include": "./js/plur",
        "includePattern": ".js$",
        "excludePattern": "(node_modules/|docs)"
    },
    "plugins": [
        "plugins/markdown"
    ],
    "opts": {
        "encoding": "utf8",
        "destination": "generated/jsdoc",
        "recurse": true,
        "verbose": true
    },
    "templates": {
        "cleverLinks": false,
        "monospaceLinks": false
    }
}
